<html>
	<body>
		<h2>Student Detail</h2>
		<script>
			var urlString,sName,sRoll,sAge;
			urlString = window.location.search;
			sName = urlString.substring(urlString.indexOf("sName")+6,urlString.indexOf("&"));

			urlString = urlString.slice(urlString.indexOf("&")+1);
			sRoll = urlString.substring(urlString.indexOf("rollNo")+7,urlString.indexOf("&"));

			urlString = urlString.slice(urlString.indexOf("&"));
			sAge = urlString.slice(urlString.indexOf("sAge")+5);

			document.write("Student Name is::<b>" + sName + "</b><br>");
			document.write("Student Roll is::<b>" + sRoll + "</b><br>");
			document.write("Student Age is::<b>" + sAge + "</b><br>");
		</script>
	</body>
</html>
